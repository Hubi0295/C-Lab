    p_short = &swar[0];
    printf ("wskaznik p_short = %p wskazuje %d\n", p_short, *p_short);
    p_short++;
    printf ("po 1 inkrem. p_short = %p wskazuje %d\n", p_short, *p_short);
    p_short = p_short + 1;
    printf ("po 2 inkrem. p_short = %p wskazuje %d\n", p_short, *p_short);
    ++p_short;
    printf ("po 3 inkrem. p_short = %p wskazuje %d\n", p_short, *p_short);
    p_short--;
    printf ("po 1 dekrem. p_short = %p wskazuje %d\n", p_short, *p_short);
